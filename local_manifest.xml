  <!-- settings -->
  <remove-project name="platform/packages/apps/Settings" />
  <project path="packages/apps/Settings" name="platform_packages_apps_settings" remote="jakdillard" revision="clean-6" />
  <!-- packages/inputmethods -->
  <remove-project name="platform/packages/inputmethods/LatinIME" />
  <project path="packages/inputmethods/LatinIME" name="platform_packages_inputmethods_latinime" remote="jakdillard" revision="clean-6" />
  <!-- vendor -->
  <!-- jakdillard -->
  <project path="vendor/jakdillard" name="platform_vendor_jakdillard" remote="jakdillard" revision="clean-6" />
