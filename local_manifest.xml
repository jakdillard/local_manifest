  <!-- sepolicy -->
  <remove-project name="platform/external/sepolicy" />
  <project path="external/sepolicy" name="platform_external_sepolicy" remote="jakdillard" revision="clean-6" />
  <!-- frameworks -->
  <!-- base -->
  <remove-project name="platform/frameworks/base" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="jakdillard" revision="clean-6" />
  <!-- kernel -->
  <remove-project name="device/huawei/angler-kernel" />
  <project path="kernel/huawei/angler" name="platform_kernel_huawei_angler" remote="jakdillard" revision="clean-6" />
  <!-- packages/apps -->
  <!-- browser -->
  <remove-project name="platform/packages/apps/Browser" />
  <project path="packages/apps/Browser" name="platform_packages_apps_browser" remote="jakdillard" revision="clean-6" />
